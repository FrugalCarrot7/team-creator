<%- include('../partials/header') %>

    <h1>Test</h1>
    <a href="/teams/new">ADD TEAM</a>
    <a href="/teams/drivers/new">Add Driver</a>
    <table id="list">
        <thead>
          <tr>

            <th>TEAM</th>
            <th>CAR</th>

          </tr>
        </thead>
        <tbody>
          <% teams.forEach(function(team) { %>
            <tr>

              <td><%= team.teamName %></td>
              <td><%= team.teamCar %></td>
              <td><a href="/teams/<%= team._id %>">DRIVERS</a></td>

            </tr>
            <% }); %>
        </tbody>
    </table>

<%- include('../partials/footer') %>